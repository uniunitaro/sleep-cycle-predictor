{
  "name": "sleep-cycle-predictor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:theme-typings": "chakra-cli tokens src/libs/chakraTheme.ts",
    "postinstall": "patch-package && npm run gen:theme-typings",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "drizzle-kit generate:mysql --config drizzle.config.ts",
    "db:push": "npm run with-env tsx src/db/migrate.ts",
    "with-env": "dotenv -e .env.local --",
    "prepare": "cd .. && husky install client/.husky",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "~2.7",
    "@edge-runtime/cookies": "^3.2.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.0.1",
    "@lukemorales/query-key-factory": "^1.2.0",
    "@mui/material": "^5.12.0",
    "@mui/x-date-pickers": "^6.1.0",
    "@planetscale/database": "^1.8.0",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.31.0",
    "@tanstack/react-query": "^4.29.1",
    "@types/node": "18.15.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "axios": "^1.3.5",
    "chakra-datetime-picker": "^1.1.7",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "drizzle-orm": "^0.27.2",
    "eslint": "8.37.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "framer-motion": "^10.10.0",
    "next": "^13.4.10",
    "next-superjson-plugin": "^0.5.9",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "simple-statistics": "^7.8.3",
    "superjson": "^1.12.3",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  },
  "overrides": {
    "chakra-datetime-picker": {
      "@chakra-ui/icons": "$@chakra-ui/icons",
      "@chakra-ui/react": "$@chakra-ui/react",
      "framer-motion": "$framer-motion",
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@chakra-ui/storybook-addon": "^5.0.1",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/nextjs": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.10",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "msw": "^1.2.3",
    "msw-storybook-addon": "^1.8.0",
    "storybook": "^7.1.0",
    "tsx": "^3.12.7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
