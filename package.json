{
  "name": "sleep-cycle-predictor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:db:push && dotenv -e .env.test -- jest",
    "e2e": "tsx e2e/setup/addDynamic.ts && (pnpm build ; tsx e2e/setup/removeDynamic.ts) && (pnpm start & dotenv -e .env.e2e -- pnpm playwright test)",
    "e2e:update": "tsx e2e/setup/addDynamic.ts && (pnpm build ; tsx e2e/setup/removeDynamic.ts) && (pnpm start & dotenv -e .env.e2e -- pnpm playwright test --update-snapshots)",
    "db:generate": "pnpm run with-env drizzle-kit generate:mysql",
    "db:push": "pnpm run with-env tsx src/db/migrate.ts",
    "test:db:push": "dotenv -e .env.test -- drizzle-kit push:mysql",
    "gen:theme-typings": "chakra-cli tokens src/libs/chakraTheme.ts",
    "postinstall": "pnpm run gen:theme-typings",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "with-env": "dotenv -e .env.local --",
    "prepare": "husky install && panda codegen",
    "analyze": "ANALYZE=true pnpm build"
  },
  "dependencies": {
    "@ark-ui/react": "^0.15.0",
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "~2.7",
    "@chakra-ui/theme-tools": "^2.1.0",
    "@ducanh2912/next-pwa": "^9.7.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.0.1",
    "@internationalized/date": "^3.5.0",
    "@mui/material": "^5.12.0",
    "@mui/x-date-pickers": "^6.1.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@park-ui/presets": "^0.18.0",
    "@planetscale/database": "^1.8.0",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.31.0",
    "@types/node": "18.15.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "aws4fetch": "^1.0.17",
    "axios": "^1.3.5",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "drizzle-orm": "^0.28.5",
    "eslint": "8.37.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "framer-motion": "^10.10.0",
    "jotai": "^2.3.1",
    "mysql2": "^3.6.0",
    "next": "13.5.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-cropper": "^2.3.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "simple-statistics": "^7.8.3",
    "typescript": "5.1.6",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@chakra-ui/storybook-addon": "^5.0.1",
    "@chakra-ui/styled-system": "^2.9.1",
    "@faker-js/faker": "^8.0.2",
    "@pandacss/dev": "^0.15.5",
    "@playwright/test": "^1.39.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/nextjs": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.3.75",
    "@swc/jest": "^0.2.28",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/uuid": "^9.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.13",
    "esbuild": "^0.19.3",
    "eslint-plugin-storybook": "^0.6.13",
